<p id="notice"><%= notice %></p>
 <div class="row">	
	 <div class="col-md-10 col-md-offset-1 col-xs-10 col-xs-offset-1">
	 	<div class="row">
	 		<div class="col-md-2 col-md-offset-1 col-xs-2  col-xs-offset-1">
	 			<%= image_tag @user.profilepic, :class => "img-responsive img-circle" %>
	 		</div>
	 		<div class="col-md-7 col-md-offset-1 col-xs-2 col-xs-offset-1">
	 			 <%= @user.email %>
	 		</div>
	 	</div>
	 </div>
</div>
<div class="row">
 <% i=0 %>	
  <% for post in @posts  %>
	 <div class="col-md-6 col-xs-6 well">
	 	<div class="row  ">
	 		<div class="col-md-6 col-xs-6 panel-image-body" style="background: url('<%= post.left.url(:medium)%>')  center ;"></div>
	 		<div class="col-md-6 col-xs-6 panel-image-body" style="background: url('<%= post.right.url(:medium)%>') center ;"></div>
	 	</div>
	 </div>
	<% i=i+1 %>	
	<% if i%2==0 %>	
		</br>	
	<% end %>	
 <% end %>
</div>